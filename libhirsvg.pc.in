prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

svgz_supported=true
css_supported=true

Name: libhirsvg
Description: library that renders svg files
Version: @VERSION@
Requires: glib-2.0 gio-2.0 gdk-pixbuf-2.0 cairo
Requires.private:
Libs: -L${libdir} -lhirsvg-@RSVG_API_MAJOR_VERSION@ -lm
Cflags: -I${includedir}/libhirsvg-@RSVG_API_VERSION@
